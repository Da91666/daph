{
  "options": {
    "daph.home": "DAPH_HOME路径",
    "daph.log-level": "info"
  },
  "nodes": [
    {
      "flag": "Spark3.dataframe.batch.connector.CommonInput",
      "config": {
        "cfg": {
          "dbtable": "user",
